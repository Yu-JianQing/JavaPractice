package aboutFIle;

import java.io.File;
import java.io.IOException;
import java.io.RandomAccessFile;

public class AccessStream {
	public static void main(String[] args) throws IOException {
		File file=new File("C:\\Users\\USER\\Desktop\\ÎÄ¼þÔÝ´æ\\IO²âÊÔ\\File2.txt");
		RandomAccessFile rAccessFile=new RandomAccessFile(file, "rw");
		
		rAccessFile.writeUTF("xxxxxxxxxxxx");
		rAccessFile.writeChars("\n");
		
		for (int i = 0; i < 20; i++) {
			rAccessFile.seek(i*4);
			System.out.println(rAccessFile.readInt());
		}
		
	}
}
