<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MMA.ProductDao">
	<select id="selectProductByPno" parameterType="Integer" resultType="Test.Product">
		select * from product
		<where>
			pno=#{pno}
		</where>
	</select>
</mapper>