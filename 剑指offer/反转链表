/*反转链表
public class ListNode {
    int val;
    ListNode next = null;

    ListNode(int val) {
        this.val = val;
    }
}*/
public class Solution {
    public ListNode ReverseList(ListNode head) {
        ListNode pre=null;
        ListNode cur=head;
        ListNode after=null;
        while(cur!=null){
            after=cur.next;
            cur.next=pre;
            pre=cur;
            cur=after;
        }
        return pre;
    }
}